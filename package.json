{
  "name": "node-typescript-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "^20.9.0",
    "pnpm": "^9.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "node dist/main.js",
    "build": "tsup src/main.ts --clean",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "9.5.0",
    "@types/node": "20.14.2",
    "eslint": "9.5.0",
    "prettier": "3.3.2",
    "tsup": "8.1.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.0.0-alpha.30",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}